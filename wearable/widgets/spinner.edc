            max: SPINNER_CENTER_IMAGE_SIZE;
            image.normal: "tw_number_controller_icon_ringtone_sound.png";
            fixed: 1 1;
         }
         description { state: "full" 0.0;
            inherit: "default" 0.0;
            /*(Changeable UI) color_class: "W172L1";*/
