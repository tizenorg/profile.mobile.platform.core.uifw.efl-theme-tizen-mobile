         description { state: "full" 0.0;
            inherit: "default" 0.0;
            /*(Changeable UI) color_class: "T0622";*/
            text.text_class: "T0622";
         }
      }
      part { name: "bottom_padding";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            rel1 {to: "content_bg"; relative: 0.0 1.0;}
