            description { state: "default" 0.0;
            }
            description { state: "hidden" 0.0;
               visible: 0;
               color: 255 255 255 0;
            }
         }
         part { name: "sb_vbar";
            type: SPACER;
            scale: 1;
            description { state: "default" 0.0;
               fixed: 1 1;
