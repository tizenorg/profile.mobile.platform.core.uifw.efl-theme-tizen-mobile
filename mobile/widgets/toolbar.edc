      }
      program { name: "shrink_hide";
         signal: "elm,state,shrink,hide";
         source: "elm";
         after: "shrink_scroll_disable";
      }
      program { name: "shrink_default";
         signal: "elm,state,shrink,default";
         source: "elm";
         after: "shrink_scroll_disable";
      }
   }
}

group { name: "elm/toolbar/base/navigationbar";
