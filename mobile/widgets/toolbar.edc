         script {
            set_int(item_state, ITEM_STATE_DISABLED);
            stop_slide();
            set_state(PART:"bg_cover", "disabled", 0.0);
            if (!get_int(icon_text))
              set_state(PART:"elm.text", "text_disabled", 0.0);
            else
