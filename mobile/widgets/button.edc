      parts {
         part { name: "button_image_base";
            scale: 1;
            type: SPACER;
            description { state: "default" 0.0;
               rel1 {
                  to_x: "padding.left";
                  to_y: "elm.text";
               }
               rel2 {
                  to_x: "padding.right";
                  to_y: "elm.text";
               }
               min: 0 BUTTON_MIN_HEIGHT;
               max: -1 BUTTON_MIN_HEIGHT;
            }
         }
         part { name: "button_image";
            scale: 1;
            description { state: "default" 0.0;
               rel1.to: "button_image_base";
