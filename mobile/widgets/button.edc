            description { state: "default" 0.0;
               rel1.to: "base";
               rel2.to: "base";
               image.normal: "core_button_bg.#";
               color: 38 107 191 255; /*(Changeable UI) color_class: "W011";*/
            }
            description { state: "pressed" 0.0;
               inherit: "default" 0.0;
