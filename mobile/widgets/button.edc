   BUTTON_ICON_STYLES("icon_expand_delete", "core_button_delete.png", "F060L3" , "F060L3P" ,"F060L3D",  BUTTON_ICON_WIDTH , BUTTON_ICON_HEIGHT)

group { name: "elm/button/base/bottom";
      script {
         public mouse_down = 0;
         public multi_down = 0;
         public text_visible = 0;
         public disabled = 0;
      }
      images {
         set {
            name: "core_button_bg.#";
            image {
               image: "SMALL/images/Controls/core_button_bg.#.png" COMP;
            }
            image {
               image: "MIDDLE/images/Controls/core_button_bg.#.png" COMP;
            }
         }
      }
      parts {
         part { name: "base";
            type: SPACER;
            scale: 1;
            description { state: "default" 0.0;
               min: BUTTON_MIN_WIDTH BUTTON_BOTTOM_MIN_HEIGHT;
               max: BUTTON_MAX_WIDTH BUTTON_BOTTOM_MIN_HEIGHT;
            }
         }
         part { name: "bg";
