         program { name: "content_cur_pushed";
            signal: "elm,state,cur,pushed,deferred";
            source: "elm";
            action: STATE_SET "leftout" 0.0;
            target: "base";
            transition: SIN_FAC NAVIFRAME_VIEW_PUSH_TRANS_TIME 1.7;
