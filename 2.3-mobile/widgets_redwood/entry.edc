	    signal: "elm,state,groupchat,enabled";
	    source: "elm";
         script {
            new st[31];
            new Float:vl;
            get_state(PART:"elm.text", st, 30, vl);
            if (!strcmp(st, "default")) {
              set_state(PART:"elm.image.bg", "groupchat_on", 0.0);
              set_state(PART:"elm.text", "groupchat_on", 0.0);
			}
            else if (!strcmp(st, "callerid_on")) {
