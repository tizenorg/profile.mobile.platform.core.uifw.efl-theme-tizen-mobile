group { name: "elm/gengrid/item/group_index/default";
   alias: "elm/gengrid/item_compress/group_index/default";
   data.item: "selectraise" "on";
   data.item: "texts" "elm.text";
   data.item: "contents" "elm.swallow.icon elm.swallow.end";
   data.item: "treesize" "20";
   parts {
      GENLIST_PART_DISCLIP_WHITE
      GENLIST_PART_BG_RECT(GENGRID_GROUP_INDEX_BG_SIZE)
      GENLIST_PART_BASE
      GENLIST_PART_PADDING_LEFT_SIZE(GENGRID_GROUP_INDEX_PADDIG_LEFT)
      GENLIST_PART_PADDING_RIGHT_SIZE(GENGRID_GROUP_INDEX_PADDIG_RIGHT)
      GENLIST_PART_PADDING_T_SIZE("elm.padding.icon.top", "base", GENGRID_GROUP_INDEX_ICON_PADDIG_TOP)
      GENLIST_PART_PADDING_B_SIZE("elm.padding.icon.bottom", "base", GENGRID_GROUP_INDEX_ICON_PADDIG_BOTTOM)    
      part { name: "bg_underline";
         type: RECT;
         mouse_events: 0;
         scale: 1;
         description { state: "default" 0.0;
           min: 0 GENGRID_GROUP_INDEX_UNDERLINE_SIZE;
           align: 0 1;
           fixed: 0 1;
           rel1
           {
              to_x: "elm.padding.left";
              relative: 1 1;
           }
           rel2
           {
              to_x: "elm.padding.right";
              relative: 0 1;
           }
           color: GENLIST_LIST_PRESS_COLOR;
           }
           description { state: "selected" 0.0;
              inherit: "default" 0.0;
              color: GENLIST_LIST_PRESS_COLOR;
           }
      }    
      
      GENLIST_PART_ICON_68_LTB("elm.swallow.icon", "elm.padding.left", "elm.padding.icon.top"
      , "elm.padding.icon.bottom")
      
      part { name: "elm.padding.icon.right";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            fixed: 1 0;
            min: GENLIST_PADDING_SIZE_DEFAULT 0;
            align: 0 0.5;
            rel1 {
               relative: 1 0;
               to: "elm.swallow.icon";
            }
            rel2 {
               relative: 1 1;
               to: "elm.swallow.icon";
            }
         }
      }
      
      part { name: "elm.padding.end.left";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            fixed: 1 0;
            min: GENLIST_PADDING_SIZE_DEFAULT 0;
            align: 1 0.5;
            rel1 {
               relative: 0 0;
               to: "elm.swallow.end";
            }
            rel2 {
               relative: 0 1;
               to: "elm.swallow.end";
            }
         }
      }
      
      GENLIST_PART_TEXT("elm.text",
         description { state: "default" 0.0;
            align: 0 1;
            rel1 { 
               relative: 1 0; 
               to_x: "elm.padding.icon.right"; 
               to_y: "bg_underline"; 
            }
            rel2 {
               relative: 0 0;
               to_x: "elm.padding.end.left";
               to_y: "bg_underline";
            }
            fixed: 0 1;
            color: GENLIST_LIST_MAIN_TEXT_DEFAULT_COLOR;
            text {
               align: 0 0.5;
               min: 1 1;
               font: "SLP:style=Regular";
               size: GENGRID_GROUP_INDEX_TEXT_SIZE;
               text_class: "list_item";
            }
         } GENLIST_DESCRIPTION_LISTITEM_OTHERS )
      
      
      GENLIST_PART_ICON_68_R("elm.swallow.end", "elm.padding.right")
      
   }
   programs {
      GENLIST_PROGRAM_DISCLIP_WHITE( target: "elm.text"; )         
   }      
} 
