            target: "controlbar_bg";
            transition: LINEAR 0.2;
         }
         program { name: "controlbar_instant_open";
            signal: "elm,state,controlbar,instant_open";
            source: "";
            script {
               emit("elm,state,prev_btn,show", "elm");
               emit("elm,state,controlbar,show", "elm");
