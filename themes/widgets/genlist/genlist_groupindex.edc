         GENLIST_PART_PADDING_LEFT_SIZE_TO("elm.padding.icon.right", "elm.icon", GENLIST_PADDING_SIZE_DEFAULT)
         GENLIST_PART_TEXT_INDEX_LIST("elm.text", "elm.padding.icon.right", "elm.padding.right")
