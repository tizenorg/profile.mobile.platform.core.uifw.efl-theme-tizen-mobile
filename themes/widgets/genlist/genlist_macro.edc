#define GENLIST_DESCRIPTION_MAP( param_x, param_y, param_z ) \
   description { state: "map_transition" 0.0; \
      inherit: "default" 0.0; \
      map { \
         perspective: "point"; \
         on: 1; \
         smooth: 1; \
         perspective_on: 1; \
         backface_cull: 1; \
         rotation { \
            center: "point"; \
            x: param_x; \
            y: param_y; \
            z: param_z; \
         } \
      } \
   } \
   description { state: "map_rotate" 0.0; \
      inherit: "default" 0.0; \
      map { \
         perspective: "point"; \
         on: 1; \
         smooth: 1; \
         perspective_on: 1; \
         backface_cull: 1; \
         rotation { \
            center: "point"; \
            x: 0; \
            y: 0; \
            z: 0; \
         } \
      } \
   } \
