         target: "elm.padding.left"; \
         target: "elm.padding.right"; \
         target: "bg_reorder_clip"; \
      ) \
      GENLIST_PROGRAM_DECORATE( \
         target: "bg_clip"; \
         target: "bottom_line_clip"; \
         target: "elm.padding.left"; \
