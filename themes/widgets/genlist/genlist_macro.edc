#define GENLIST_PROGRAM_MAP_ROTATE( EXPAND_SECOND, CONTRACT_SECOND, TARGET ) \
   program { name: "rotaion_transition"; \
      signal: "flip_item"; \
      action: STATE_SET "map_transition" 0.0; \
      after: "rotation"; \
      TARGET \
   } \
   program { name: "rotation"; \
      action: STATE_SET "map_rotate" 0.0; \
      transition: LINEAR EXPAND_SECOND; \
      after: "rotation_end"; \
      TARGET \
   } \
   program { name: "rotation_end"; \
      action: STATE_SET "default" 0.0; \
      TARGET \
   } \
   program { name: "rotation_transition2"; \
      signal: "elm,state,contract_flip"; \
      action: STATE_SET "map_rotate" 0.0; \
      after: "rotation2"; \
      TARGET \
   } \
   program { name: "rotation2"; \
      action: STATE_SET "map_transition" 0.0; \
      transition: LINEAR CONTRACT_SECOND; \
      after: "rotation3"; \
      TARGET \
   } \
   program { name: "rotation3"; \
      action: STATE_SET "hide" 0.0; \
      TARGET \
   }

