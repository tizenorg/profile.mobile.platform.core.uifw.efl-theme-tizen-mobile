      after: "map_contracting"; \
   } \
   program { name: "map_contracting"; \
      action: STATE_SET "map_contracted" 0.0; \
      transition: LINEAR 0.3; \
      target: "bg"; \
      TARGET \
      after: "map_contract_end"; \
   } \
