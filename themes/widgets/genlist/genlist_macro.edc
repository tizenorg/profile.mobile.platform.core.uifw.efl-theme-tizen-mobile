 #define GENLIST_DESCRIPTION_ENVELOP( PART, PARAM ) \
      description { state: "default" 0.0;\
         rel1 { \
            relative: 0 0; \
            to: PART; \
         }\
         rel2 {\
            relative: 1 1;\
            to: PART; \
         }\
         PARAM\
      }

//      |   top  |
// ---------------------
// left |  HERE  | right
// ---------------------
//      | bottom |
#define GENLIST_DESCRIPTION( left, right, top, bottom, param ) \
      description { state: "default" 0.0;\
         rel1 { \
            relative: 1 1; \
            to_x: left; \
            to_y: top; \
         }\
         rel2 {\
            relative: 0 0;\
            to_x: right;\
            to_y: bottom;\
         }\
         param\
      }

