group {
    name: "elm/colorselector/colorplane/default";
    images {
        image: "00_color_picker_color_box.png" COMP;
        image: "00_color_picker_select_kit.png" COMP;
    }
    parts{
       part {
          name: "base";
          type: RECT;
          scale: 1;
          description {
             state: "default" 0.0;
             min: 0 COLORPLANE_MIN_HEIGHT;
             color: 0 0 0 0;
            }
        }
        part {
           name: "padding_left_top";
           type: SPACER;
           scale: 1;
           description {
              state: "default" 0.0;
              min: COLORSELECTOR_COLORPLANE_PADDING_OUTER;
              align: 0.0 0.0;
              fixed: 1 1;
              rel2.relative: 0.0 0.0;
           }
        }
        part {
           name: "padding_right_bottom";
           type: SPACER;
           scale: 1;
           description {
              state: "default" 0.0;
              min: COLORSELECTOR_COLORPLANE_PADDING_OUTER;
              align: 1.0 1.0;
              fixed: 1 1;
              rel1.relative: 1.0 1.0;
           }
        }
        part {
           name: "outer";
           type: SPACER;
           scale: 1;
           description {
              state: "default" 0.0;
              fixed: 1 1;
              rel1.to: "padding_content_left_top";
              rel2.to: "padding_content_right_bottom";
           }
        }
        part {
            name: "colorplane_bg";
            type: IMAGE;
            scale: 1;
            description {
               state: "default" 0.0;
                aspect_preference: SOURCE;
                rel1 {
                   to:"padding_left_top";
                   relative: 1.0 1.0;
                }
                rel2 {
                   to:"padding_right_bottom";
                   relative: 0.0 0.0;
                }
                image.normal: "00_color_picker_color_box.png";
            }
        }
        part {
           name: "padding_content_left_top";
           type: SPACER;
           scale: 1;
           description {
              state: "default" 0.0;
              min: COLORSELECTOR_COLORPLANE_PADDING_COLORPLANE_BG;
              align: 1.0 1.0;
              fixed: 1 1;
              rel1.to: "colorplane_bg";
              rel2 {
                 to: "colorplane_bg";
                 relative: 0.0 0.0;
              }
           }
        }
        part {
           name: "padding_content_right_bottom";
           type: SPACER;
           scale: 1;
           description {
              state: "default" 0.0;
              min: COLORSELECTOR_COLORPLANE_PADDING_COLORPLANE_BG;
              align: 0.0 0.0;
              fixed: 1 1;
              rel1 {
                 to: "colorplane_bg";
                 relative: 1.0 1.0;
              }
              rel2.to: "colorplane_bg";
           }
        }
        part {
           name: "events";
           type: RECT;
           description {
              state: "default" 0.0;
              rel1.to: "colorplane_bg";
              rel2.to: "colorplane_bg";
              color: 0 0 0 0;
           }
        }
        part {
           name: "elm.touch";
           type: IMAGE;
           scale: 1;
           mouse_events: 0;
           dragable {
              confine: "colorplane_bg";
              x: 1 1 1;
              y: 1 1 1;
           }
           scale: 1;
           description {
              state: "default" 0.0;
              min: COLORSELECTOR_COLORPLANE_CONTROL_SIZE;
              fixed: 1 1;
              visible: 0;
              image {
                 normal: "00_color_picker_select_kit.png";
                 border: 4 4 4 4;
                 border_scale: 1;
              }
           }
           description {
              state: "visible" 0.0;
              inherit: "default" 0.0;
              visible:1;
           }
        }
        script {
           public mouse_down_done = 0;
        }
    } //end parts
    programs {
       program {
          name: "mouse_down";
          signal: "mouse,down,1";
          source: "events";
          script {
             new x1, y1;
             new x2, y2, w, h;
             get_mouse(x1, y1);
             get_geometry(PART:"colorplane_bg", x2, y2, w, h);
             if ( w == 0) w = 1;
             if ( h == 0) h = 1;
             new x_offset = x1 - x2;
             new y_offset = y1 - y2;
             new Float:d_x = (1.0 *(x_offset)) / (w - 1);
             new Float:d_y = (1.0 * (y_offset)) / (h - 1) ;
             set_drag(PART:"elm.touch", d_x, d_y);
             set_int(mouse_down_done, 1);
             set_state(PART:"elm.touch", "visible", 0.0);
             emit("changed", "");
          }
       }
       program {
          name: "mouse_up";
          signal: "mouse,up,1";
          source: "events";
          script {
             set_int(mouse_down_done, 0);
          }
       }
       program {
          name: "show";
          signal: "show";
          source: "elm";
          script {
             set_state(PART:"elm.touch", "visible", 0.0);
          }
       }
       program {
          name: "hide";
          signal: "hide";
          source: "elm";
          script {
             set_state(PART:"elm.touch", "default", 0.0);
          }
       }
       program {
          name: "mouse_move";
          signal: "mouse,move";
          source: "events";
          script {
             new x1, y1;
             new x2, y2, w, h;
             if (get_int(mouse_down_done)) {
                get_mouse(x1, y1);
                get_geometry(PART:"colorplane_bg", x2, y2, w, h);
                if ( w == 0) w = 1;
                if ( h == 0) h = 1;
                if (x1 < x2) x1 = x2;
                if (x1 > (x2 + w - 1)) x1 = x2 + w - 1;
                if (y1 < y2) y1 = y2;
                if (y1 > (y2 + h - 1)) y1 = y2 + h -1;
                new x_offset = x1 - x2;
                new y_offset = y1 - y2;
                new Float:d_x = (1.0 *(x_offset)) / (w - 1);
                new Float:d_y = (1.0 * (y_offset)) / (h - 1) ;
                set_drag(PART:"elm.touch", d_x, d_y);
                emit("changed", "");
              }
          }
       }
    } //end programs
} //end group
