				}
			}
			part { name: "elm.swallow.arrow_down";
				type: SWALLOW;
				mouse_events: 0;
				scale: 1;
				clip_to: "ctxpopup_clip";
				dragable {
					x: 1 1 0;
					y: 1 1 0;
					confine: "arrow_area_down";
				}
				description {
					state: "default" 0.0;
					min: 40 40;
					fixed: 1 1;
					visible: 1;
				}
			}
			part { name: "elm.swallow.arrow_left";
				type: SWALLOW;
				mouse_events: 0;
				scale: 1;
				clip_to: "ctxpopup_clip";
				dragable {
					x: 1 1 0;
					y: 1 1 0;
					confine: "arrow_area_left";
				}
