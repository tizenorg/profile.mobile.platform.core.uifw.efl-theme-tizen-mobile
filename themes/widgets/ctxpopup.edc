					align: 0.5 0;
					rel1 {
						relative: 1 CTXPOPUP_ARROW_DOWN_OFFSET;
						to:"ctxpopup_frame_left_bottom";
					}
					rel2 {
						relative: 0 CTXPOPUP_ARROW_DOWN_OFFSET;
						to_x: "ctxpopup_frame_right_bottom";
