		programs {
			program { name: "show";
				signal: "elm,state,show";
				source: "elm";
				action: STATE_SET "visible" 0.0;
				transition: LINEAR 0.25;
				target: "ctxpopup_clip";
			}
			program { name: "show_up";
				signal: "elm,state,show,up";
