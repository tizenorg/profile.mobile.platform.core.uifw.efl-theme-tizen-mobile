					max: CTXPOPUP_HORIZONTAL_SEPARATOR_MAX_SIZE_INC;
					visible: 1;
				}
			description {
					state: "vertical" 0.0;
					color: CTXPOPUP_SEPARATOR_COLOR_INC;
					align: 0.5 0;
					min: 0 1;
					max: 999999 1;
					visible: 1;
