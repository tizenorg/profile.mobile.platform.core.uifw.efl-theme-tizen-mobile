      to_x: "focus_left_padding"; \
      to_y: "top_padding";\
      relative: 1.0 1.0; \
   } \
   rel2 { \
      to_x: "clear_padding"; \
      to_y: "bottom_padding"; \
      relative: 0.0 0.0; \
   }

