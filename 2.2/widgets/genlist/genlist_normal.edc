   group { GENLIST_NAME("editfield/title/default")
      alias: "elm/layout/editfield/title/default";

      PROLOG_LIST_NORMAL
      data.item: "contents" "elm.icon.entry";
      data.item: "texts" "elm.text";
      parts {
         GENLIST_PART_BASE
