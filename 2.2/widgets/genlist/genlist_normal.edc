   group { GENLIST_NAME("1text.2icon.3/default")
      PROLOG_LIST_NORMAL
      data.item: "texts" "elm.text";
      data.item: "contents" "elm.icon.1 elm.icon.2";

      parts {
         GENLIST_PART_BASE
         PART_LIST_BG_NORMAL
         PART_LIST_BOTTOMLINE
