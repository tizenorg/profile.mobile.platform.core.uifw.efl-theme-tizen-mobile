            fixed: 1 1;
            text {
              style: "popup_title_style2";
              size: POPUP_DOW_DEFAULT_FONT_SIZE_INC;
              align: 0.0 0.5;
            }
            rel1 {
               relative: 1.0 1.0;
               to_x: "padding_after_icon";
               to_y: "title_left_top_padding";
            }
            rel2 {
               relative: 0.0 1.0;
               to: "title_right_bottom_padding";
            }
