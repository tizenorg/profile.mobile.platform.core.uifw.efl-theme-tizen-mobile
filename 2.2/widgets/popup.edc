            inherit: "default" 0.0;
            rel1 {
               relative: 0.0 1.0;
               to: "elm.bg.title";
            }
            rel2 {
               relative: 1.0 1.0;
               to: "base";
            }
         }
         description { state: "show_action_area" 0.0;
            inherit: "default" 0.0;
            rel1 {
               relative: 0.0 0.0;
               to: "base";
